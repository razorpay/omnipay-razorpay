{
    "name": "razorpay/omnipay-razorpay",
    "description": "Razorpay Omnipay 3.0 plugin for accepting payments",

    "keywords": [
      "razorpay", "payments", "india",
      "payment gateway", "wallets", "netbanking"
    ],
    "authors": [
        {
            "name": "Mayank Amencherla",
            "email": "mayank.amencherla@razorpay.com",
            "role": "Developer"
        }
    ],
    "support": {
      "email": "support@razorpay.com"
    },
    "homepage": "https://razorpay.com",
    "type": "library",
    "autoload": {
        "psr-4": { "Omnipay\\Razorpay\\" : "src/" }
    },
    "require": {
       "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "^3",
        "squizlabs/php_codesniffer": "^3",
        "phpro/grumphp": "^0.14.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "license": [
        "MIT"
    ]
}
